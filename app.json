{
  "expo": {
    "name": "Issy",
    "slug": "issy-resident-app",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F1A1E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.issy.resident",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:app.joinissy.com",
        "applinks:joinissy.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "Usa Face ID para acceder a tu cuenta de forma segura",
        "NSCameraUsageDescription": "ISSY necesita acceso a la camara para escanear codigos QR",
        "NSPhotoLibraryUsageDescription": "ISSY necesita acceso a tus fotos para subir imagenes",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.issy.resident",
              "issy"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F1A1E"
      },
      "package": "com.issy.resident",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.joinissy.com",
              "pathPrefix": "/join"
            },
            {
              "scheme": "https",
              "host": "joinissy.com",
              "pathPrefix": "/join"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "scheme": "issy",
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#AAFF00"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Permite usar Face ID para acceso seguro"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "ISSY necesita acceso a tus fotos para subir imagenes",
          "cameraPermission": "ISSY necesita acceso a la camara para tomar fotos"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "ISSY necesita acceso a la camara para escanear codigos QR"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "supabaseUrl": "https://fxawqxquinldcxkvwoet.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImZ4YXdxeHF1aW5sZGN4a3Z3b2V0Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjI3NDAwNDYsImV4cCI6MjA3ODMxNjA0Nn0.Nu0AXZf9iy0kjPYi7V6acrhtiHuv30HkNGECb7AhgLk",
      "eas": {
        "projectId": "c4a103bf-1ca0-4901-817c-af9dc40325e8"
      }
    },
    "owner": "jbuesoo",
    "runtimeVersion": "1.0.0",
