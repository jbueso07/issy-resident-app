{
  "expo": {
    "name": "ISSY Resident",
    "slug": "issy-resident-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.issy.resident",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.issy.resident"
    },
    "scheme": "com.issy.resident",
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "expo-font",
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "c4a103bf-1ca0-4901-817c-af9dc40325e8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c4a103bf-1ca0-4901-817c-af9dc40325e8",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}