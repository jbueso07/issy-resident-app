{
  "expo": {
    "name": "ISSY Resident",
    "slug": "issy-resident-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.issy.resident",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:app.joinissy.com",
        "applinks:joinissy.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.issy.resident",
              "issy",
              "com.googleusercontent.apps.844659689774-9dmfp7uhgh4kj9pl7na3lfec8hdfk0pl"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.issy.resident",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.joinissy.com",
              "pathPrefix": "/join"
            },
            {
              "scheme": "https",
              "host": "joinissy.com",
              "pathPrefix": "/join"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "scheme": "com.issy.resident",
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "expo-font",
      "expo-apple-authentication",
      
    ],
    "extra": {
      "eas": {
        "projectId": "c4a103bf-1ca0-4901-817c-af9dc40325e8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c4a103bf-1ca0-4901-817c-af9dc40325e8",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}